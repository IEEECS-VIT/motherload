<div>
    <ul>
            <div id="events2">
                <div class="container">
                    <div class="row">
                        <div class="col s12 l12 m12 wow fadeInRight display-categories-{{categoriesFlag}}" data-wow-duration="2s" data-wow-delay="0ms">
                            <h1><span style="color:white">Events</span></h1>
                        </div>
                        <div ng-repeat="category in categories" ng-click="showEvents($index)" class="col s12 m4 l4 wow fadeInDown display-categories-{{categoriesFlag}}" data-wow-duration="2s" data-wow-delay="0ms">
                            <article class="material-card Indigo">
                                <h2 class="z-depth-2"><span>{{category.name}}</span><strong><i class="fa fa-fw fa-star"></i>Know More...</strong></h2>
                                <div class="mc-content">
                                    <div class="img-container waves-effect waves-light z-depth-3">
                                        <img src="{{category.url}}"></div></div><div class="mc-footer">
                                    </div>
                            </article>
                        </div>
                        <div class="col s12 m12 l12 wow fadeInLeft display-events-{{eventsFlag}}" data-wow-duration="1000ms" data-wow-delay="0ms"><a href="/index#/events" ng-click="showCategories()" class="back left-align"><i class="fa fa-arrow-left" aria-hidden="true"></i></a><h3 class="center-align" style="color: white">{{categories[index].name}}</h3></div>
                        <div ng-repeat="event in events" class="col s12 m6 l4 wow fadeInDown display-events-{{eventsFlag}}" data-wow-duration="1000ms" data-wow-delay="0ms">
                            <div class="card events z-depth-2">
                                <div class="card-heading">
                                    <h5>{{event.subcat}}<br><small>{{event.club}}</small></h5>
                                    <a href="#eventsmodal"class="btn-floating btn-small waves-effect waves-light red right"><i class="material-icons">add</i></a>
                                </div>
                            </div>
                        </div>

                    </div>
                        <div id="bottom_fade">

                        </div>
                        </div>
                </div>
            </div>

    </ul>
</div>
