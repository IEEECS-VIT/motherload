
<div ng-app>
<div id="organizers" class="organizers">
<h1 class="center" style="color: white;">Organizers</h1>
    <div class="container">
        <div class="row">
            <div class="col s12 m12 l12">
                <div class="card z-depth-4 wow fadeInDown" data-wow-duration="1000ms" data-wow-delay="100ms">
                    <div class="card-content">
                        <div class="container">
                            <div class="row">
                                <div class="col s12 l4 m4" ng-repeat="org in organizer">
                                    <img class="circle responsive-img" src="{{org.url}}" alt="{{org.name}}">
                                    <p>{{org.name}}</p>
                                    <p>{{org.committee}}</p>
                                    <br>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
        </div>
    </div>
</div>
</div>
</div>
