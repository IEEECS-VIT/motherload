        .text-copy {
            fill: none;
            stroke: white;
            stroke-dasharray: 7% 28%;
            stroke-width: 3px;
            -webkit-animation: stroke-offset 15s infinite linear;
            animation: stroke-offset 7s infinite linear;
        }
        .text-copy:nth-child(1) {
            stroke: #360745;
            stroke-dashoffset: 7%;
        }
        .text-copy:nth-child(2) {
